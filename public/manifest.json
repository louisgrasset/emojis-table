{
  "manifest_version": 2,
  "name": "Emojis table",
  "version": "2.0",
  "description": "Get the right emojis from your browser in any tab!",
  "homepage_url": "https://github.com/louisgrasset/emojis-table",
  "browser_action": {
    "default_icon": {
      "16": "./icons/icon-16.png",
      "32": "./icons/icon-32.png"
    },
    "default_title": "Emojis Table",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "/icons/icon-16.png",
    "32": "/icons/icon-32.png",
    "48": "/icons/icon-48.png",
    "96": "/icons/icon-96.png",
    "128": "/icons/icon-128.png",
    "256": "/icons/icon-256.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+X"
      }
    }
  },
  "permissions": [
    "storage"
  ],
  "author": "Louis Grasset <hello@louisgrasset.fr> (louisgrasset.fr)"
}