{
  "name": "emojis-table",
  "private": true,
  "version": "3.2.2",
  "type": "module",
  "scripts": {
    "dev:chromium": "BROWSER=chromium node scripts/buildManifest.mjs && vite",
    "predev": "mkdir -p ./dist && npm run inject:polyfill && npm run build:emojis",
    "_build": "tsc && vite build && npm run inject:polyfill",
    "build:chromium": "npm run build:emojis && BROWSER=chromium node scripts/buildManifest.mjs && npm run _build",
    "build:firefox": "npm run build:emojis && BROWSER=firefox node scripts/buildManifest.mjs && npm run _build",
    "build:emojis": "node scripts/buildEmojiData.mjs",
    "preview": "vite preview",
    "inject:polyfill": "cp ./node_modules/webextension-polyfill/dist/browser-polyfill.min.js ./dist/",
    "lint": "eslint src/ --ext .tsx,.ts --fix"
  },
  "dependencies": {
    "emojilib": "^3.0.11",
    "log-update": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "unicode-emoji-json": "^0.4.0",
    "vite-plugin-svgr": "^4.1.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.15",
    "@types/chrome": "^0.0.249",
    "@types/react": "^18.2.35",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}
